!
! Cisco IOS-XE Boot Configuration Template
! Generated by Ansible on {{ ansible_date_time.iso8601 }}
! Device: {{ inventory_hostname }}
!
{% if install_mode | default(false) %}
! Install Mode Configuration
boot system {{ target_firmware_version }}
{% else %}
! Bundle Mode Configuration  
boot system bootflash:{{ target_firmware_filename }}
{% endif %}
!
! Boot parameters
boot-start-marker
boot system switch all {{ target_firmware_version | default(target_firmware_filename) }}
boot-end-marker
!
! Configuration register
config-register 0x2102
!
! End of boot configuration
!